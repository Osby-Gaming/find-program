{
  "name": "find-program",
  "module": "src/index.ts",
  "type": "module",
  "version": "0.2.1",
  "exports": {
    "types": "./src/index.ts",
    "import": "./dist/esm/index.js",
    "require": "./dist/cjs/index.cjs"
  },
  "scripts": {
    "build": "concurrently \"bun run build-node\" \"bun run build-bun\" \"bun run build:declaration\"",
    "build-browser": "bun build ./src/index.ts --outdir ./dist/browser --target browser",
    "build-node": "bun build ./src/index.ts --outdir ./dist/cjs --target node --format cjs --packages external",
    "build-bun": "bun build ./src/index.ts --outdir ./dist/esm --target bun --packages external",
    "dev": "concurrently \"bun run dev-node\" \"bun run dev-bun\" \"bun run dev:declaration\"",
    "dev-browser": "bun build ./src/index.ts --outdir ./dist/browser --target browser --watch",
    "dev-node": "bun build ./src/index.ts --outdir ./dist/cjs --target node --packages external --format cjs --watch",
    "dev-bun": "bun build ./src/index.ts --outdir ./dist/esm --target bun --packages external --watch",
    "build:declaration": "tsc ./src/index.ts --emitDeclarationOnly --declaration --outFile ./dist/esm/index.js",
    "dev:declaration": "tsc ./src/index.ts --emitDeclarationOnly --declaration --outFile ./dist/esm/index.js --watch",
    "demo": "concurrently \"bun run dev-bun\" \"bun ./demo/index.ts\""
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "concurrently": "^9.2.1",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "regedit": "^5.1.4"
  }
}
